{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@nodejs-fullstack-template/api",
  "packageManager": "pnpm@10.10.0",
  "exports": {
    ".": "./src/index.ts"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun --watch ./scripts/typegen.ts",
    "typegen": "bun ./scripts/typegen.ts",
    "build": "pnpm typegen && tsc",
    "lint": "pnpm typegen && biome lint . && tsc"
  },
  "dependencies": {
    "@nodejs-template/docs-openapi": "workspace:^",
    "@zodios/core": "^10.9.6",
    "openapi-fetch": "^0.13.4",
    "openapi-react-query": "^0.2.9"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "openapi-types": "^12.1.3",
    "openapi-typescript": "^7.4.1",
    "openapi-zod-client": "^1.18.3",
    "openapi3-ts": "^4.4.0",
    "raw-loader": "^4.0.2"
  }
}
